  <table id="summary-table" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>TOPIC ID</th>
            <th>DOCUMENT</th>
            <th>SEARCH ENGINE</th>
            <th>SCORE</th>
            <th>INDEX</th>
            <th>ASSIGNED DATE</th>
            <th>IS RELATED</th>
        </tr>
    </thead>
    <tbody>
        {{#each assignments as |asg|}}
            <tr aria-describedby="topic-row">
                <td class="mailbox-name">{{#each asg.topic.topicid as |tpid|}}{{tpid}}{{/each}}</td>
                <td>{{#each asg.topic.document as |doc|}}{{doc}}{{/each}}</td>
                <td>{{#each asg.topic.searchEngine as |sengine|}}{{sengine}}{{/each}}</td>
                <td>{{#each asg.topic.score as |score|}}{{score}}{{/each}}</td>
                <td>{{#each asg.topic.index as |ind|}}{{ind}}{{/each}}</td>
                 <td>{{asg.assignedDate}}</td>
                <td aria-describedby="{{asg.isRelated}}">{{asg.isRelated}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>